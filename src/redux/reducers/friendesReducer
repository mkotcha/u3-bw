import { ADD_FRIEND } from "../actions";

const initialState = {
  content: [],
};

const mainPagePostsReducer = (state = initialState, action) => {
  switch (action.type) {
    case ADD_FRIEND:
      return {
        ...state,
        content: [...state.content, action.payload],
      };
    default:
      return state;
  }
};

export default mainPagePostsReducer;
